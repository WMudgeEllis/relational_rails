<form action="/car_lots" method='post'>
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>enter new car lot name</p>

  <input type="text" name="name"/> <br/>
  <p>being cleaned?</p>

  <select id="being_cleaned" name="being_cleaned">
    <option value="pick one">pick one below</option>
    <option value=true>true</option>
    <option value=false>false</option>
  </select>

  <p>what is the square footage of the car lot?</p>
  <input type="integer" name="lot_area">
  <input type="submit" name="submit">
</form>
