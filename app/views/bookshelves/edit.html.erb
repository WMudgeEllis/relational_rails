

<%= form_with url: "/bookshelves/#{@bookshelves.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :full? %>
  <%= form.select :full, ['pick one below', 'true', 'false'] %>
  <%= form.label :capacity %>
  <%= form.number_field :capacity %>
  <%= form.submit "submit"%>

<% end %>
