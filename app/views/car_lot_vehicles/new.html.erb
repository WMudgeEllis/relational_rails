<%= form_with url:"/car_lots/#{@car_lot.id}/vehicles", method: :post, local: true do |form|  %>
  <%= form.label :name %><br>
  <%= form.text_field :name %><br>
  <%= form.label :sold? %><br>
  <%= form.select :sold, ["pick one","true","false"] %><br>
  <%= form.label :price %><br>
  <%= form.number_field :price %><br>
  <%= form.submit "submit" %>
<% end %>